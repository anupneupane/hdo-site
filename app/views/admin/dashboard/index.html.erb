<div class="container-fluid">
  <div class="row-fluid">
    <h3>Mine saker</h3>
    <ul>
      <% current_user.issues.each do |issue| %>
      <li><%= link_to issue.title, issue %></li>
      <% end %>
    </ul>
  </div>

  <div class="row-fluid">
    <div class="span6">
      <h3><%= Vote.model_name.human(count: 2).capitalize %></h3>
      <%= render 'votes_by_date' %>
    </div>

    <div class="span6">
      <h3><%= ParliamentIssue.model_name.human(count: 2).capitalize %></h3>
      <%= render 'parliament_issues_by_status' %>
    </div>
  </div>

</div>