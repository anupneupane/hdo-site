
    <meta property="og:title" content="<%=t 'app.title' %>" />
    <meta property="og:site_name" content="<%=t 'app.title' %>" />
    <meta property="og:description" content="<%=t 'app.opengraph.description' %>" />
    <meta property="og:type" content="non_profit" />
    <meta property="og:url" content="<%= "http://#{request.host}#{request.fullpath}" %>" />
    <meta property="og:image" content="<%= File.join(root_url, asset_path('og_logo.png')) %>" />
    <% t('app.opengraph.admins').split(':').each do |fbid| %>
      <meta property="fb:admins" content="<%= fbid %>" />
    <% end %>
